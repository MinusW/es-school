<div id="<%= dom_id classroom %>">
  <p>
    <strong>Name:</strong>
    <%= classroom.name %>
  </p>

  <p>
    <strong>Class type:</strong>
    <%= classroom.class_type_id %>
  </p>

  <p>
    <strong>Room:</strong>
    <%= classroom.room_id %>
  </p>

  <p>
    <strong>Teacher:</strong>
    <%= classroom.teacher_id %>
  </p>

  <p>
    <strong>Is archived:</strong>
    <%= classroom.is_archived %>
  </p>

</div>
